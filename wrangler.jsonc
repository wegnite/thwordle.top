    "NEXT_PUBLIC_BASE_URL": "https://thwordle.top",
    "NEXT_URL": "https://thwordle.top",
    "VERCEL_URL": "thwordle.top",
    "DISABLE_IMAGE_OPTIMIZATION": "true",
    "NEXT_PUBLIC_DEMO_WEBSITE": "false",
    "NEXT_PUBLIC_GOOGLE_ANALYTICS_ID": "G-MM928ZVYZB",
